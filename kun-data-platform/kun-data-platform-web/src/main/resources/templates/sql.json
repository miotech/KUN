{
  "name": "SparkSQL",
  "templateType": "sql",
  "templateGroup": "development",
  "jarPath": "",
  "operator": {
    "name": "sparkSql-operator",
    "description": "Spark SQL Operator",
    "className": "com.miotech.kun.workflow.operator.SparkSQLOperator"
  },
  "defaultValues": {
    "livyHost": "${ dataplatform.livy.host }",
    "variables": "{}",
    "jars": "${ dataplatform.lineage.analysis.jar }",
    "sparkConf": "{\"spark.sql.queryExecutionListeners\":\"za.co.absa.spline.harvester.listener.SplineQueryExecutionListener\", \"spark.hadoop.spline.hdfs_dispatcher.address\":${dataplatform.lineage.output.path}, \"spark.fs.s3a.access.key\":${dataplatform.s3.access.key}, \"spark.fs.s3a.secret.key\":${dataplatform.s3.secret.key}}"
  },
  "displayParameters": [{
    "name": "sparkSQL",
    "displayName": "sql",
    "type": "sql",
    "required" : true
  }, {
      "name": "sparkConf",
      "type": "keyvalue",
      "required": false,
      "displayName": "Extra Spark Configuration"
    }],
  "renderClassName": "com.miotech.kun.dataplatform.common.tasktemplate.renderer.SparkConfigTaskTemplateRender"
}